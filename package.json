{
  "name": "ay-sensors",
  "description": "爱用 神策 SDK",
  "scripts": {
    "build": "pnpm -r --filter=./packages/* run build",
    "build-*": "pnpm -r --filter=ay-sensors-* run build",
    "dev": "pnpm -r --filter=./packages/* run dev",
    "build-ay-sensors-direct": "pnpm -r --filter ay-sensors-direct run build",
    "dev-ay-sensors-direct": "pnpm -r --filter ay-sensors-direct run dev",
    "lint": "eslint ./packages/ --ext .ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "chalk": "4",
    "cross-env": "^7.0.3",
    "dts-bundle": "^0.7.3",
    "eslint": "7.x",
    "eslint-config-ay": "^1.0.4",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-require-jsdoc-except": "^1.5.0",
    "lodash": "^4.17.21",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.10.0"
  }
}
